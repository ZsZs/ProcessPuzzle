{
  "name": "@processpuzzle/main",
  "version": "0.2.1",
  "license": "MIT",
  "scripts": {
    "run-mock-backend": "npx json-server --watch mock-backend/db.json",
    "run-http-server": "http-server dist/processpuzzle-ui/browser --proxy http://localhost:8080?",
    "serve-processpuzzle-ui": "nx run processpuzzle-ui:serve --configuration=de-DE",
    "i18n-processpuzzle-ui": "nx extract-i18n processpuzzle-ui",
    "build-all-ci": "nx run-many -t build --configuration=production --no-cloud",
    "build-base-entity": "nx run base-entity:build",
    "build-base-entity-ci": "nx run base-entity:build --configuration=production --no-cloud",
    "build-util": "nx run util:build",
    "build-util-ci": "nx run util:build --configuration=production --no-cloud",
    "lint-all": "nx run-many -t lint --no-cloud",
    "lint-all-ci": "nx run-many -t lint --configuration=ci --no-cloud",
    "lint-base-entity": "nx run base-entity:lint --no-cloud",
    "lint-test-build": "nx run-many --targets=lint,test,build --coverage --no-cloud",
    "publish-all": "nx release --skip-publish --no-cloud",
    "publish-all-ci": "nx release publish --dry-run --no-cloud",
    "release-all-ci": "nx release publish --skip-publish --no-cloud --dry-run",
    "release-check": "nx release plan:check --no-cloud",
    "test-all": "nx run-many -t test --coverage --no-cloud",
    "test-all-ci": "nx run-many -t test --exclude=processpuzzle-ui --configuration=ci --no-cloud",
    "test-base-entity": "nx run base-entity:test --coverage --no-cloud"
  },
  "private": false,
  "dependencies": {
